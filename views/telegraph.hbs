{{> header}}

<div class="headlines">

    <div class="theathletic">
        <h1>TheTelegraph</h1>
        {{#if article}}
            {{#each article}}
                <h4><a href="{{url}}">{{title}}</a></h4>
            {{/each}}
        {{else}}
            <h1>No headline found</h1>
        {{/if}}
    </div>

</div>